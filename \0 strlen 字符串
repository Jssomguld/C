#include <stdio.h>

//字符串 - "abcdiskzos"
//数据在计算机上存储时的时候，存储的是2进制
//ASCII 编码

int main()
{
	char arr1[] = "abc";//数组
	//"abc" - 'a' 'b' 'c' '\0' - '\0'字符串的结束标志
	char arr2[] = {'a','b','c'};
	//'a' 'b' 'c'
	printf("%s\n",arr1);
	printf("%s\n",arr2);
	//strlen - string length - 计算字符串长度
	printf("%d\n",strlen(arr1));//3
	printf("%d\n",strlen(arr2));//15 (加'\0'后为3)
	return 0;
}
